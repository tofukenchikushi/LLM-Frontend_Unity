.func1UI--invisible {
    transition-duration: 0.7s;
    opacity: 0;
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: rgb(255, 0, 243);
    translate: 0 100%;
}

.func1UI--visible {
    transition-duration: 0.7s;
    opacity: 1;
    translate: 0 0;
}

.centerLogo {
    width: 300px;
    height: 300px;
    max-width: 400px;
    max-height: 400px;
    min-width: 200px;
    min-height: 200px;
    background-color: rgb(0, 134, 255);
    transition-duration: 2s;
    position: absolute;
    top: 60%;
    transition-timing-function: ease;
}

.centerLogo--down {
    translate: 0 15%;
}
